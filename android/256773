      classrelation 2878853 // mAirplaneModeReceiver (<unidirectional association>)
	relation 2878853 --->
	  a role_name "mAirplaneModeReceiver" init_value " new BroadcastReceiver() {
        @Override
        public void onReceive(Context context, Intent intent) {
            if (Settings.Global.getInt(context.getContentResolver(),
                    Settings.Global.AIRPLANE_MODE_ON, 0) == 0) {
                startScanning();
                requestCellScan();
            }
