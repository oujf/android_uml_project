	  param in name "toDegrees" explicit_type "float"
	  param in name "pivotXType" explicit_type "int"
	  param in name "pivotXValue" explicit_type "float"
	  param in name "pivotYType" explicit_type "int"
	  param in name "pivotYValue" explicit_type "float"
	cpp_decl "    ${comment}${inline}${name}${(}${)}${volatile}${throw};
"
	cpp_def "${comment}${inline}${class}::${name}${(}${)}${volatile}${throw} {
  ${body}}
"
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${synchronized}${name}${(}${t0} ${p0}, ${t1} ${p1}, ${t2} ${p2}, ${t3} ${p3}, ${t4} ${p4}, ${t5} ${p5}${)}${throws}${staticnl}{
  ${body}}
