      operation 6121093 "openTypedAssetFile"
	public return_type class_ref 1054213 // AssetFileDescriptor
	nparams 3
	  param inout name "uri" type class_ref 961157 // Uri
	  param in name "mimeTypeFilter" explicit_type "String"
	  param inout name "opts" type class_ref 960005 // Bundle
	nexceptions 1
	  explicit_exception "FileNotFoundException"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
"
	cpp_def "${comment}${inline}${type} ${class}::${name}${(}${)}${const}${volatile}${throw}${staticnl}{
  ${body}}
"
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}@NonNull ${t0} ${p0}, @NonNull ${t1} ${p1}, @Nullable ${t2} ${p2}${)}${throws}${staticnl}{
  ${body}}
