      operation 6122245 "applyBatch"
	public return_type class_ref 1054597 // ContentProviderResult
	nparams 1
	  param inout name "operations" explicit_type "ArrayList<ContentProviderOperation>"
	nexceptions 1
	  exception class_ref 1054725 // OperationApplicationException
